namespace ConsoleApp3
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            int flag = 0;
            for (int i = 1; i < n; i++)
            {
                if (n % i == 0)
                {
                    Console.WriteLine("ne pirminis");
                    flag++;
                    break;
                }

            }
            if (flag == 0)
            {
                Console.WriteLine("skaicius pirminis");
            }
            
        }
    }
}
